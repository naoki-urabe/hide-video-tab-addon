{
    "manifest_version": 3,
    "name": "Google Video Tab Hider",
    "version": "1.0",
    "description": "Google検索の動画タブを非表示にする拡張機能",
    "permissions": ["scripting"],
    "host_permissions": ["https://www.google.com/search*"],
    "content_scripts": [
      {
        "matches": ["https://www.google.com/search*"],
        "js": ["content.js"],
        "css": ["style.css"],
        "run_at": "document_end"
      }
    ]
  }
  